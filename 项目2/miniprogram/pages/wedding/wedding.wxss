page {
  margin: 0;
  padding: 0; 
  font-size: 16px;
}

/* 隐藏滚动条 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* button 去掉边框 */

#loginbtn::after {
  border: none;
}

/* button 去掉背景 */

#loginbtn {
  background-color: transparent;
}

.page_content{ 
  background-color:#e74c3c;  
}

.page_num {
  display: flex;
  flex-direction: row; 
  justify-content: center;
} 

.page_num_content{
  width: 750rpx;
  height: 100%;
  background-color:#e74c3c; 
  display: flex;
  flex-direction: column;
  justify-content: flex-start;  
} 

/* 模态窗 */

.modelwindow {
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  z-index: 99;
  overflow: hidden;
  background-color: #5a6a7a;
  display: flex;
  justify-content: center;
  align-items: center;
}

.loader_img{
  width: 256rpx;
  height: 254rpx;  
  position: relative;
}

.loader_text{
  width: 256rpx;
  height: 254rpx;  
  text-align: center;
  line-height: 254rpx;
  font-size:45rpx;
  letter-spacing: 10rpx;
  position: relative; 
}

.loader{
  width: 300rpx;
  height: 300rpx; 
  border-radius: 180rpx;
  border: 0rpx solid gold;
  border-color: gold transparent transparent gold;
  border-width: 12rpx 12rpx 0rpx 0rpx; 
  position: absolute;
  box-shadow: 0rpx 0rpx 33rpx gold;
  animation: animation_circle 3s linear infinite;  
}

.circle{
  width: 40rpx;
  height: 40rpx; 
  border-radius: 20rpx; 
  position: relative;
  left:78%;
  top : 2%;
  background-color: gold; 
}


 
@keyframes animation_circle {
  0% { 
    transform: rotate(0deg)
  }
 100%{ 
    transform: rotate(360deg)
  }
}

 
@keyframes animation_out {
  0% {   
    transform: translateY(0rpx); 
  }
 100%{   
    transform: translateY(-1000px); 
  }
}

/* 往上图 */

.update_icon { 
  position: fixed;
  z-index: 51;
  bottom: 0px;
  left: 45%;
  width: 10%;
  height: 10%;
  background-repeat: no-repeat;
  background-position: 50% 95%; 
  background-image: url("data:image/png;base64,");
  animation: animation_icon 2s linear infinite;
}

 
@keyframes animation_icon {
  0% {
    opacity: 0;
    transform: translateY(0px)
  }

  50% {
    opacity: 1;
     transform: translateY(-15px)
  }

    70% {
    opacity: 0;
     transform: translateY(-20px)
  }

    100% {
    opacity: 0; 
  }
}

/* 音乐 */

.music { 
  position: fixed;
  top: 0;
  right: 0;
  width: 60px;
  height: 60px;
  z-index: 51;
}

@keyframes animation_music_move {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

/* 创建按钮 */

