page {
  margin: 0;
  padding: 0;
}

/* 隐藏滚动条 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* button 去掉边框 */

#loginbtn::after {
  border: none;
}

/* button 去掉背景 */

#loginbtn {
  background-color: transparent;
}

