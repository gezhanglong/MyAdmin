page {
  margin: 0;
  padding: 0; 
  font-size: 16px;
}

/* 隐藏滚动条 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* button 去掉边框 */

#loginbtn::after {
  border: none;
}

/* button 去掉背景 */

#loginbtn {
  background-color: transparent;
}

.page_content{ 
  background-color:#e74c3c;  
}

.page_num {
  display: flex;
  flex-direction: row; 
  justify-content: center;
} 

.page_num_content{
  width: 750rpx;
  height: 100%;
  background-color:#e74c3c; 
  display: flex;
  flex-direction: column;
  justify-content: flex-start;  
} 

/* 模态窗 */

.modelwindow {
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  z-index: 50;
  overflow: hidden;
}

/* 往上图 */

.update_icon {
  position: fixed;
  z-index: 51;
  bottom: 0px;
  left: 25%;
  width: 50%;
  height: 20%;
  background-repeat: no-repeat;
  background-position: 50% 95%; 
  background-image: url("data:image/png;base64,");
  animation: animation_icon 3s linear infinite;
}

 
@keyframes animation_icon {
  0% {
    opacity: 0.2;
  }

  50% {
    opacity: 1;
  }
}

/* 音乐 */

.music { 
  position: fixed;
  top: 0;
  right: 0;
  width: 60px;
  height: 60px;
  z-index: 51;
}

@keyframes animation_music_move {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

/* 第一屏 */


 
.page_1_name {  
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-content:center;
  width: 100%;
  height: 140rpx; 
  font-size: 48rpx;
  margin-top: 100rpx;
  background-position: 60% 50%;
  background-size:100% 100%;
  background-repeat: no-repeat; 
  background-image: url("data:image/png;base64,");
}

.page_1_name view { 
  line-height: 100rpx; 
  color:#2a3a4a;

}

@keyframes animation_page_1_name {
  0% {
    opacity: 0;
    visibility: hidden;
  }

  100% {
    opacity: 1;
    visibility: visible;
  }
}

.page_1_img1 {
  width: 100%;
  height: 460rpx;
  text-align:center;
  padding-top:100rpx; 
}

.page_1_img2 image {
  width: 100%;
  height:460rpx;
}

@keyframes animation_page_1_img1 {
  0% {
    opacity: 0;
    visibility: hidden;
  }

  50% {
    opacity: 0;
    visibility: hidden;
  }

  100% {
    opacity: 1;
    visibility: visible;
  }
}

.page_1_img2 {
  width: 100%;
  height: 124rpx;
  text-align:center; 
  margin-top:30rpx;

}

.page_1_img2 image {
  width: 600rpx;
  height:124rpx;
}

@keyframes animation_page_1_img2 {
  0% {
    opacity: 0.2;
    transform: scale(0.5, 0.5);
  }

  50% {
    opacity: 1;
    transform: scale(1.2, 1.2);
  }

  100% {
    opacity: 0.2;
    transform: scale(0.5, 0.5);
  }
}

.page_1_gull {
  position: absolute;
  display: flex;
  flex-direction: row;
  align-items: center;
  left: -1000px;
}

.page_1_headurl {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50rpx;
}

.page_1_nickname {
  color: #666;
  font-size: 24rpx;
  margin-left: 10rpx;
  text-align: left;
}

.page_1_wishtext {
  color: #ccc;
  font-size: 32rpx;
  margin-left: 10rpx;
}

@keyframes animation_page_1_gull {
  0% {
    transform: translateX(2000px);
  }

  100% {
    transform: translateX(-1000px);
  }
}

/* 第二屏 */
 
.page_2_img1, .page_2_img2 {
  width: 100%;
  height: 200rpx;
  margin: 30rpx 0px 0px 0px;
  padding-top: 60rpx;
  text-align: center;
}

.page_2_img1 image {
  width: 600rpx;
  height: 200rpx;
}

.page_2_img2 image {
  width: 300rpx;
  height: 140rpx;
}

@keyframes animation_page_2_img {
  0% {
    transform: translateY(0rpx);
  }

  20% {
    transform: translateY(20rpx);
  }

  40% {
    transform: translateY(0rpx);
  }

  70% {
    transform: translateY(20rpx);
  }

  100% {
    transform: translateY(0rpx);
  }
}

.page_2_text view {
  height: 70rpx;
  line-height: 40rpx;
  font-size: 32rpx;
  color: #1a2a3a;
  text-align: center;
}

@keyframes animation_page_2 {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 0;
  }
}

@keyframes animation_page_2_text {
  0% {
    opacity: 0;
  }

  10% {
    opacity: 1;
  }

  100% {
    opacity: 1;
  }
}

/* 第三屏 */
 
 

.page_3_content_img {
  display: flex;
  justify-content: space-around;
  
}

.page_3_img {
  width: 300rpx;
  height: 350rpx;  
  border-radius: 20rpx;
}

@keyframes animation_page_3_img {
  0% {
    transform:rotate(0deg);
  }

  100% {
   transform: rotate(360deg);
  }
}

/* 第四屏 */
 
.page_4_img {
  position: absolute;
  width: 500rpx;
  height: 750rpx;
  top: 15%;
  left: 15%;
}

@keyframes animation_page_4 {
  0% {
    transform: translateX(1000rpx);
  }

  100% {
    transform: translateX(0rpx);
  }
}

@keyframes animation_page_4_left {
  0% {
    transform: translateX(0rpx);
    z-index: 10;
    opacity: 1;
  }

  50% {
    transform: translateX(-1000rpx);
    opacity: 0;
  }

  100% {
    transform: translateX(0rpx);
    z-index: 9;
    opacity: 1;
  }
}

@keyframes animation_page_4_right {
  0% {
    transform: translateX(0rpx);
    z-index: 10;
    opacity: 1;
  }

  50% {
    transform: translateX(1000rpx);
    opacity: 0;
  }

  100% {
    transform: translateX(0rpx);
    z-index: 9;
    opacity: 1;
  }
}

.page_4_left {
  left: 0;
  top: 50%;
  transform: rotate(-90deg);
}

.page_4_right {
  right: 0;
  top: 50%;
  transform: rotate(90deg);
}

.page_4_icon {
  position: absolute;
  z-index: 51;
  width: 80rpx;
  height: 60rpx;
  background-repeat: no-repeat;
  background-position: 50% 95%;
  background-image: url("data:image/png;base64,");
}

/* 第五屏 */
 
.page_5_img1 {
  width: 100%;
  height: 380rpx;
  text-align: center;
  margin-top: 100rpx;
}

.page_5_img1 image {
  width: 400rpx;
  height: 380rpx;

}

@keyframes animation_page_5_img1_1 {
  0% {
    transform: translateY(-400rpx);
  }

  20% {
    transform: translateY(300rpx);
  }

  100% {
    transform: translateY(0rpx);
  }
}

@keyframes animation_page_5_img1_2 {
  0% {
    transform: rotate(0deg);
  }

  20% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

.page_5_img2 {
  width: 100%;
  height: 120rpx;
  margin: 60rpx 0rpx;
    text-align: center;
}

.page_5_img2 image {
  width: 600rpx;
  height: 120rpx;
}

.page_5_img3 {
  width: 100%;
  height: 300px;
    text-align: center;
      margin-top: 50rpx;
}

.page_5_img3 image {
  width: 480rpx;
  height: 80rpx;
}

.page_5_text view {
  height: 70rpx;
  line-height: 40rpx;
  font-size: 40rpx;
    text-align: center;
}

@keyframes animation_page_5_text {
  0% {
    transform: scale(1, 1);
  }

  50% {
    transform: scale(1.2, 1.2);
  }

  100% {
    transform: scale(1, 1);
  }
}

/* 第六屏 */

 
.page_6_phone {
  text-align: center;
  
}

.page_6_phonenum { 
  color: #1a2a3a;
  width: 260rpx;
  height: 84rpx;
  line-height: 90rpx;
  display: inline-block; 
  position: relative;
  z-index: 51;
  font-size: 40rpx;
}

.page_6_phonename {
  height: 84rpx; 
  line-height: 90rpx;
  display: inline-block;
  font-size: 40rpx;
}

.page_6_phonename_imag1 {
  width: 140rpx;
  height: 140rpx; 
}


/* 第七屏 */

 
.page_7_img1 {
  width: 55%;
  height: 360rpx;
  float: left;
  text-align: right;
  margin-top: 50rpx;
}

.page_7_img1 image {
  width: 260rpx;
  height: 360rpx;
}

@keyframes animation_page_7_img1 {
  0% {
    transform: translateX(0rpx);
  }

  50% {
    transform: translateX(1000rpx);
  }

  80% {
    transform: translateX(0rpx);
  }

  90% {
    transform: translateX(1000rpx);
  }

  100% {
    transform: translateX(0rpx);
  }
}

.page_7_img2 {
  width: 50%;
  height: 360rpx;
  display: inline-block;
  text-align: left;
  margin-left: -40rpx;
  margin-top: 50rpx;
}

.page_7_img2 image {
  width: 280rpx;
  height: 360rpx;
}

@keyframes animation_page_7_img2 {
  0% {
    transform: translateX(1000rpx);
  }

  50% {
    transform: translateX(1000rpx);
  }

  80% {
    transform: translateX(0rpx);
  }

  90% {
    transform: translateX(-40rpx);
  }

  100% {
    transform: translateX(0rpx);
  }
}

.page_7_img3 {
  width: 100%;
  height: 600rpx;
  margin-top: 40rpx;
}

.page_7_img3 image {
  width: 760rpx;
  height: 500rpx;
}

.page_7_btn{
  position:absolute;
   z-index:52;
   bottom:0;
   background-color:#e67e22;
}