/* 第七屏 */

.page_7 {
  text-align: center;
}

.page_7_img1 {
  width: 55%;
  height: 180px;
  float: left;
  text-align: right;
  margin-top: 50px;
}

.page_7_img1 image {
  max-width: 130px;
  max-height: 180px;
}

@keyframes animation_page_7_img1 {
  0% {
    transform: translateX(0px);
  }

  50% {
    transform: translateX(500px);
  }

  80% {
    transform: translateX(0px);
  }

  90% {
    transform: translateX(50px);
  }

  100% {
    transform: translateX(0px);
  }
}

.page_7_img2 {
  width: 50%;
  height: 180px;
  display: inline-block;
  text-align: left;
  margin-left: -20px;
  margin-top: 50px;
}

.page_7_img2 image {
  max-width: 140px;
  max-height: 180px;
}

@keyframes animation_page_7_img2 {
  0% {
    transform: translateX(500px);
  }

  50% {
