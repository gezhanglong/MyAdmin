} 

.page_2_img2 image{
  width: 150px;
  height: 70px; 
} 

@keyframes animation_page_2_img
{
   0%	{transform:translateY(0px);}  
   20% {transform:translateY(10px);}  
   40% {transform:translateY(0px);}  
   70%	{transform:translateY(10px);}  
   100% {transform:translateY(0px);}   
} 


.page_2_text view{
  height: 35px;
  line-height: 20px;
  font-size: 16px;  
}

@keyframes animation_page_2
{
  0%	{opacity: 0;}  
   100%	{opacity: 0;}   
} 

@keyframes animation_page_2_text
{
   0%	{opacity: 0;}  
   10%	{opacity: 1;} 
   100%	{opacity: 1;} 
} 


/* 第五屏 */

.page_5{
  display: flex;
  flex-direction: column;
  justify-content:space-evenly;
  text-align: center;
}

.page_5_img1{
  width: 100%;
  height: 180px;
 
}

.page_5_img1 image{  
  max-width: 170px;
  max-height: 160px; 
}

@keyframes animation_page_5_img1_1
{
   0%	{transform: translateY(-200px);}  
   20%	{transform: translateY(150px); } 
   100%	{transform: translateY(0px);} 
} 

@keyframes animation_page_5_img1_2
{
   0%	{transform: rotate(0deg);}  
   20%	{transform: rotate(0deg); } 
   100%	{ transform: rotate(360deg);} 
} 

.page_5_img2{
  width: 100%;
  height: 60px;
}

.page_5_img2 image{
  width: 300px;
  height: 60px;
}


.page_5_img3{
   width: 100%;
  height: 150px;
  
}

.page_5_img3 image{
 width: 240px;
  height: 40px; 
}

.page_5_text view{
  height: 35px;
  line-height: 20px;
  font-size: 20px;  
}

@keyframes animation_page_5_text
{
   0%	{transform: scale(1,1)}   
   50%	{transform: scale(1.2,1.2)}   
   100%	{ transform: scale(1,1);} 
} 


/* 第七屏 */ 

.page_7{ 
  text-align: center;
}

.page_7_img1{
  width: 55%;
  height: 180px; 
  float: left;
  text-align:right; 
  margin-top:60px; 
  
}

.page_7_img1 image{  
  max-width: 130px;
  max-height: 180px; 
}

@keyframes animation_page_7_img1
{
   0%	{transform: translateX(0px);}  
   50%	{transform: translateX(500px); } 
   80%	{transform: translateX(0px);} 
   90%	{transform: translateX(50px);} 
    100%	{transform: translateX(0px);} 
} 

.page_7_img2{
  width: 50%;
  height: 180px; 
  display: inline-block;
  text-align:left;
  margin-left:-20px;
  margin-top:60px;

}

.page_7_img2 image{  
  max-width: 140px;
  max-height: 180px; 
}

@keyframes animation_page_7_img2
{
   0%	{transform: translateX(500px);}  
   50%	{transform: translateX(500px); } 
   80%	{transform: translateX(0px);} 
    90%	{transform: translateX(-20px);} 
    100%	{transform: translateX(0px);} 
} 

.page_7_img3{
  width: 100%;
  height: 300px;
  margin-top:20px;
}

.page_7_img3 image{
  width: 380px;
  height:250px;
}