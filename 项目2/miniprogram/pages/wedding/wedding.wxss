/* 模态窗 */

.modelwindow {
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  z-index: 1;
  overflow: hidden;
}

/* 往上图 */

.update_icon {
  position: fixed;
  z-index: 2;
  bottom: 0px;
  left: 25%;
  width: 50%;
  height: 20%;
  background-repeat: no-repeat;
  background-position: 50% 95%;
  background-image: url("data:image/png;base64,");
  animation: animation_icon 3s linear infinite;
}

@keyframes animation_icon {
  0% {
    opacity: 0.2;
  }

  50% {
    opacity: 1;
  }
}

/* 音乐 */

.music {
  position: fixed;
  top: 0;
  right: 0;
  width: 60px;
  height: 60px;
  z-index: 4;
}

@keyframes animation_music_move {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

/* 第一屏 */

.page_1 {
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
  position: relative;
}

.page_1_name {
  width: 100%;
  height: 70px;
  margin: 0px 0px 25px 0px;
  font-size: 24px;
  background-repeat: no-repeat;
  background-position-x: 45%;
  background-image: url("data:image/png;base64,");
}

.page_1_name view {
  display: inline-block;
  line-height: 50px;
}

@keyframes animation_page_1_name {
  0% {
    opacity: 0;
    visibility: hidden;
  }

  100% {
    opacity: 1;
    visibility: visible;
  }
}

.page_1_img1 {
  width: 100%;
  height: 280px;
}

@keyframes animation_page_1_img1 {
  0% {
    opacity: 0;
    visibility: hidden;
  }

  50% {
    opacity: 0;
    visibility: hidden;
  }

  100% {
    opacity: 1;
    visibility: visible;
  }
}

.page_1_img2 {
  width: 100%;
  height: 62px;
}

.page_1_img2 image {
  width: 300px;
  height: 62px;
}

@keyframes animation_page_1_img2 {
  0% {
    opacity: 0.2;
    transform: scale(0.5, 0.5);
  }

  50% {
    opacity: 1;
    transform: scale(1.2, 1.2);
  }

  100% {
    opacity: 0.2;
    transform: scale(0.5, 0.5);
  }
}


 

.page_1_gull {
  position: absolute;
  display: flex;
  flex-direction: row;
  align-items: center;
  left: -1000px;
}

.page_1_headurl {
  width: 40px;
  height: 40px;
  border-radius: 25px;
}

.page_1_nickname {
  color: #666;
  font-size: 28rpx;
  margin-left: 5px;
  text-align:left; 
}

.page_1_wishtext {
  color: #ccc;
  font-size: 32rpx;
  margin-left: 5px;
}


@keyframes animation_page_1_gull {
  0% {
    transform: translateX(2000px);
  }

  100% {
    transform: translateX(-1000px);
  }
}

/* 第二屏 */

.page_2 {
  text-align: center;
  margin-top: 80px;
}

.page_2_img1, .page_2_img2 {
  width: 100%;
  height: 100px;
  margin: 20px 0px 0px 0px;
  padding-top: 40px;
}

.page_2_img1 image {
  width: 300px;
  height: 100px;
}

.page_2_img2 image {
  width: 150px;
  height: 70px;
}

@keyframes animation_page_2_img {
  0% {
    transform: translateY(0px);
  }

  20% {
    transform: translateY(10px);
  }

  40% {
    transform: translateY(0px);
  }

  70% {
    transform: translateY(10px);
  }

  100% {
    transform: translateY(0px);
  }
}

.page_2_text view {
  height: 35px;
  line-height: 20px;
  font-size: 16px;
}

@keyframes animation_page_2 {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 0;
  }
}

@keyframes animation_page_2_text {
  0% {
    opacity: 0;
  }

  10% {
    opacity: 1;
  }

  100% {
    opacity: 1;
  }
}

/* 第三屏 */

.page_3 {
  position: relative;
}

.page_3_gull {
  position: absolute;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.page_3_headurl {
  width: 40px;
  height: 40px;
  border-radius: 25px;
}

.page_3_nickname {
  color: #666;
  font-size: 28rpx;
  margin-left: 5px;
}

.page_3_wishtext {
  color: #ccc;
  font-size: 32rpx;
  margin-left: 15px;
}

@keyframes animation_page_3_gull {
  0% {
    transform: translateX(1000px);
  }

  100% {
    transform: translateX(-1000px);
  }
}

/* 第四屏 */

.page_4 {
  display: flex;
  justify-content: center;
  align-items: Center;
  position: relative;
}

.page_4 image {  
  position: absolute;
  width: 300px;
  height: 450px; 
  margin-top: -60px;  
}

@keyframes animation_page_4 {
  0% {
    transform: translateX(500px); 
  }
   100% {
    transform: translateX(0px); 
  }
}

@keyframes animation_page_4_left {
  0% {
    transform: translateX(0px);
    z-index: 0;
    opacity: 1;
  }

  50% {
    transform: translateX(-500px); 
     opacity: 0;
  }

   100% {
    transform: translateX(0px);
    z-index: -1;
     opacity: 1;
  }
}

@keyframes animation_page_4_right {
  0% {
    transform: translateX(0px);
    z-index: 0; 
    opacity: 1;
  }

  50% {
    transform: translateX(500px); 
     opacity: 0;
  }

   100% {
    transform: translateX(0px);
    z-index: -1; 
    opacity: 1;
  }
}

.page_4_left {  
  right: 40%; 
  transform: rotate(-90deg);
  }

  .page_4_right {  
  left:40%;
  transform: rotate(90deg);
  }

.page_4_icon{
  position: relative;
  z-index: 2;
  width: 60px;
  height: 90px;
  background-repeat: no-repeat;
  background-position: 50% 95%;
  background-image: url("data:image/png;base64,");
  
}

/* 第五屏 */

.page_5 {
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
  text-align: center;
}

.page_5_img1 {
  width: 100%;
  height: 180px;
}

.page_5_img1 image {
  max-width: 170px;
  max-height: 160px;
}

@keyframes animation_page_5_img1_1 {
  0% {
    transform: translateY(-200px);
  }

  20% {
    transform: translateY(150px);
  }

  100% {
    transform: translateY(0px);
  }
}

@keyframes animation_page_5_img1_2 {
  0% {
    transform: rotate(0deg);
  }

  20% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

.page_5_img2 {
  width: 100%;
  height: 60px;
}

.page_5_img2 image {
  width: 300px;
  height: 60px;
}

.page_5_img3 {
  width: 100%;
  height: 150px;
}

.page_5_img3 image {
  width: 240px;
  height: 40px;
}

.page_5_text view {
  height: 35px;
  line-height: 20px;
  font-size: 20px;
}

@keyframes animation_page_5_text {
  0% {
    transform: scale(1, 1);
  }

  50% {
    transform: scale(1.2, 1.2);
  }

  100% {
    transform: scale(1, 1);
  }
}

/* 第六屏 */

