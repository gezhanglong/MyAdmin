page{
  margin: 0;
  padding: 0;   
} 

/* 隐藏滚动条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
} 

.page_content{
  background-repeat:repeat; 
  background-image: url("data:image/jpeg;base64,")
}


/* 模态窗 */
.modelwindow{
  position: fixed; 
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%; 
  z-index: 1;
  overflow: hidden; 
}

 /* 往上图 */
.update_icon{ 
 position: fixed;
 z-index: 2;
bottom: 0px; 
left: 25%;
 width: 50%;
  height: 20%; 
  background-repeat: no-repeat;
  background-position: 50% 95%;
  background-size: 30% 30%;
  background-image: url("data:image/png;base64,");
  animation: animation_icon 3s linear infinite;
}

@keyframes animation_icon
{
   0%		{opacity: 0.2} 
50%		{opacity: 1} 
} 


/* 第一屏 */

.page_1{
  display: flex;
  flex-direction: column;
 justify-content: center;   
  text-align: center;
}
.page_1_name{
  width: 100%;
  height: 70px; 
  margin: 0px 0px 25px 0px;
  font-size: 24px;
  background-repeat: no-repeat; 
  background-position-x: 45%;
  background-image: url("data:image/png;base64,"); 
}

.page_1_name view{
  display: inline-block;
  line-height: 50px; 
  
}

@keyframes animation_page_1_name
{
   0%	{opacity: 0;visibility: hidden;}  
   100%	{opacity: 1;visibility: visible;} 
} 

.page_1_img1{
  width: 100%;
  height: 280px;   
}

 @keyframes animation_page_1_img1
{
   0%	{opacity: 0;visibility: hidden;}  
   50%	{opacity: 0;visibility: hidden;}
   100%	{opacity: 1;visibility: visible;} 
} 

.page_1_img2{
  width: 100%;
  height: 62px;  
}

.page_1_img2 image{
  width: 300px;
  height: 62px;
}

 
@keyframes animation_page_1_img2
{
   0%		{opacity: 0.2;transform: scale(0.5,0.5)} 
50%		{opacity: 1;transform: scale(1.2,1.2)} 
100%		{opacity: 0.2;transform: scale(0.5,0.5)} 
} 


