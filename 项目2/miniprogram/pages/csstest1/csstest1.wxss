.content{
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  width: 100%;
  height: 100%;
}

.content-box{
  border: 1rpx solid #e5e5e5;
  background-color: #FFFFFF;
  width: 60%;
  height: 500rpx;
  border-radius: 5px; 
  margin-top: 40rpx; 
  text-align: center;
  position: relative;
}
 
.box-des{
  font-size: 24rpx;
  padding: 20rpx;
  color: #999999;
}

.box-bnt{
  position: absolute;
  bottom: 0;
  width: 100%;
  display: none;
}

.box-bnt-on{
  position: absolute;
  bottom: 0;
  width: 100%; 
}

.box-img1{ 
  background-position: 7px -358px;
  }

  .box-img2{ 
  background-position: 13px -495px;
  }

  .box-img3{ 
  background-position: 0 -569px;
  }
.box-img{ 
  display: inline-block;
  margin: 40rpx 0; 
