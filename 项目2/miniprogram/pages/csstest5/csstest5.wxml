<view style=' width:{{windowWidth}}px;  height:{{windowHeight}}px;background-color:black;' catchtouchmove='onCatchtouchmove'>
  <view wx:for='{{seat}}' wx:key='key'>
    <view class='{{item.starclass}}' style='top: {{item.top}}rpx;left: {{item.left}}rpx;{{item.animation}}'></view>
  </view>
  <view class='pop' style='top:{{poptop}};'>
    <view class='pop_top' bindtap='onopen'>{{poptext}}</view>
    <view class='pop_content'>
      <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" circular="{{circular}}" vertical="{{vertical}}" interval="{{interval}}" duration="{{duration}}" previous-margin="{{previousMargin}}px" next-margin="{{nextMargin}}px" style='height:100%;width:100%;'
        bindchange='onchange'>
        <swiper-item>
          <view class='templet'>
            <image class='templetimg' src='https://7a6c-zltest-d3262f-1258495345.tcb.qcloud.la/my-image.jpg?sign=a8a0fff68c17b56cfdc9870bb90b8f8e&=1553736254'></image>
            <view class='templettext'>大红婚礼邀请函模板</view>
          </view>
        </swiper-item>
        <swiper-item>
           <view class='templet'>
            <image class='templetimg' src='https://7a6c-zltest-d3262f-1258495345.tcb.qcloud.la/my-image.jpg?sign=a8a0fff68c17b56cfdc9870bb90b8f8e&=1553736254'></image>
            <view class='templettext'>大红婚礼邀请函模板</view>
          </view>
        </swiper-item> 
      </swiper>
    </view>
  </view>
</view>